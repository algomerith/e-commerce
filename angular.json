{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"cli": {
		"packageManager": "npm",
		"analytics": false
	},
	"newProjectRoot": "projects",
	"projects": {
		"prix-dassaut": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/prix-dassaut",
			"sourceRoot": "projects/prix-dassaut/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-architects/native-federation:build",
          "options": {
            "styles": [
              "projects/prix-dassaut/src/material-theme.scss"
            ]
          },
					"configurations": {
						"production": {
							"target": "prix-dassaut:esbuild:production"
						},
						"development": {
							"target": "prix-dassaut:esbuild:development",
							"dev": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-architects/native-federation:build",
					"options": {
						"target": "prix-dassaut:serve-original:development",
						"rebuildDelay": 500,
						"dev": true,
						"cacheExternalArtifacts": false,
						"port": 0
					}
				},
				"test": {
					"builder": "@angular/build:unit-test"
				},
				"esbuild": {
					"builder": "@angular/build:application",
					"options": {
						"browser": "projects/prix-dassaut/src/main.ts",
						"tsConfig": "projects/prix-dassaut/tsconfig.app.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/prix-dassaut/public"
							}
						],
						"styles": [
							"projects/prix-dassaut/src/styles.css"
						],
						"polyfills": [
							"es-module-shims"
						]
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve-original": {
					"builder": "@angular/build:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "prix-dassaut:esbuild:production"
						},
						"development": {
							"buildTarget": "prix-dassaut:esbuild:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4200
					}
				}
			}
		},
		"team": {
